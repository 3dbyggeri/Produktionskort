<%= f.inputs :class => "tableize inputs contains_sub_inserts" do %>
  <%= f.input :unit_number %>
  <%= f.input :load_number %>
  <%= f.input :gate_number %>
  <%= f.input :crane_number %>
  <%= f.input :planned_delivery_date, :as => :string %>
  <%= f.input :craning_time %>
  <%= f.input :ordered_delivery_date, :as => :string %>
  <%= f.input :delivery_conditions %>
  <%= f.input :return_policy %>
  <%= link_to_remove_fields "Slet", f %>
<% end %>

